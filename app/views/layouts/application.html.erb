<% content_for :page_title do %>
  <%= Settings.dsds.app_title %>
<% end %>
<% content_for :stylesheets do %>
  <%= stylesheet_link_tag('application', media: 'all') %>
  <!--[if IE 6]>
  <%= stylesheet_link_tag "govuk_elements_extensions_ie6", media: "all" %>
  <![endif]-->
  <!--[if IE 7]>
  <%= stylesheet_link_tag "govuk_elements_extensions_ie7", media: "all" %>
  <![endif]-->
  <!--[if IE 8]>
  <%= stylesheet_link_tag "govuk_elements_extensions_ie8", media: "all" %>
  <![endif]-->
<% end %>
<% content_for :javascripts do %>
  <%= javascript_include_tag 'application' %>
<% end %>
<% content_for :head do %>
  <%= csrf_meta_tag %>
  <%= render partial: 'shared/ga' %>
<% end %>
<%= content_for :cookie_message, "" %>
<%= content_for :js_gt_ie, '6' %>
<%# TODO: convert banner to i18n? %>
<% content_for :content_override do %>
  <main id="page-container" role="main">
    <div class="phase-banner">
      <p>
        <span>This is a new service - <%= link_to('your feedback', config_item(:feedback_url)) %> will help us to improve it.  If you need help with using the online service, we can offer <%= link_to('technical help', '#') %>.</span>
      </p>
    </div>
    <%= yield %>
  </main>
<% end %>
<% content_for :proposition_links do %>
  <%= render 'shared/user_profile_links' %>
<% end %>

<% content_for :footer_support_links do %>
  <li>
    <ul>
      <li>
        <%= link_to t('help'), :help %>
      </li>
      <li>
        <%= link_to t('accessibility'), :accessibility %>
      </li>
      <li>
        <%= link_to t('cookies'), :cookies %>
      </li>
      <li>
        <%= link_to t('terms'), :terms %>
      </li>
    </ul>
  </li>
<% end %>

<% content_for :body_end do %>
  <%= render js_partial rescue nil %>
<% end %>

<%= render template: 'layouts/moj_template' %>

